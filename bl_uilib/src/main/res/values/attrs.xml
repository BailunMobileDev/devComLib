<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--汇查查通用Button-->
    <declare-styleable name="BailunButton">
        <attr name="bl_btn_style" format="enum">
            <enum name="only_text" value="0" />
            <enum name="with_loading" value="1" />
            <enum name="with_image" value="2" />
        </attr>
        <attr name="bl_btn_content" format="string" />
        <attr name="bl_btn_enabled" format="boolean" />
        <attr name="bl_btn_bg" format="color|reference" />
        <attr name="bl_tv_color" format="reference" />
        <attr name="bl_tv_text_size" format="dimension"/>
        <attr name="bl_tv_loading_color" format="reference" />
        <attr name="bl_btn_loading_bg" format="reference" />
        <attr name="bl_iv_res" format="reference" />
        <attr name="bl_iv_width" format="dimension" />
        <attr name="bl_iv_height" format="dimension" />
    </declare-styleable>
</resources>